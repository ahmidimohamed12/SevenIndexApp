<?xml version="1.0" encoding="UTF-8" ?>
<ContentPage
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="sevenindex.Views.onboarding.MyPage_parties">
    <ContentPage.Content>
        <StackLayout>
            <StackLayout VerticalOptions="CenterAndExpand" >
                  <Frame x:Name="frm1"  WidthRequest="270" HorizontalOptions="CenterAndExpand" BackgroundColor="#3A4FA7">
                <StackLayout>
                    <Label x:Name="txtstudent" FontSize="Title" HorizontalOptions="Center" TextColor="White" FontAttributes="Bold"/>
                </StackLayout>
            </Frame>
            <Frame x:Name="frm2" WidthRequest="270" HorizontalOptions="CenterAndExpand" BackgroundColor="#3A4FA7">
                <StackLayout>
                    <Label x:Name="txtprof" FontSize="Title" HorizontalOptions="Center" TextColor="White" FontAttributes="Bold" />
                </StackLayout>
            </Frame>
            <Frame x:Name="frm3" WidthRequest="270" HorizontalOptions="CenterAndExpand" BackgroundColor="#3A4FA7">
                <StackLayout>
                    <Label x:Name="txtuser" TextColor="White" FontSize="Title" HorizontalOptions="Center" FontAttributes="Bold"/>
                </StackLayout>
            </Frame>
  
            </StackLayout>
                </StackLayout>
    </ContentPage.Content>
</ContentPage>
